{
  "name": "@astra-void/auth-kit",
  "version": "0.7.0",
  "description": "Next.js authentication with passkey support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "maintainers": [
    {
      "name": "astra-void",
      "email": "dev@astra-void.xyz"
    }
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.mjs"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.mjs"
    },
    "./store": {
      "types": "./dist/store/index.d.ts",
      "import": "./dist/store/index.mjs"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "import": "./dist/react/index.mjs"
    },
    "./providers": {
      "types": "./dist/providers/index.d.ts",
      "import": "./dist/providers/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly --declaration --outDir dist && pnpm pack",
    "dev": "tsup src/index.ts --watch --format esm --dts --outDir dist",
    "lint": "eslint",
    "test": "jest --config ./jest.config.js"
  },
  "keywords": [
    "jwt",
    "authkit",
    "nextjs",
    "passkey",
    "authentication",
    "webauthn",
    "react"
  ],
  "author": "astra-void",
  "license": "MIT",
  "peerDependencies": {
    "@simplewebauthn/browser": "^13.1.0",
    "@simplewebauthn/server": "^13.1.1",
    "argon2": "^0.43.0",
    "bcrypt": "^6.0.0",
    "next": "^15.3.3",
    "nodemailer": "^7.0.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/css": "^0.9.0",
    "@eslint/js": "^9.30.1",
    "@eslint/json": "^0.12.0",
    "@eslint/markdown": "^6.6.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.16.2",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.8",
    "babel-jest": "^29.7.0",
    "eslint": "^9.30.1",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "next": "^15.3.5",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.36.0"
  },
  "dependencies": {
    "@noble/hashes": "^1.8.0",
    "jose": "^6.0.11",
    "ts-prune": "^0.10.3",
    "uuid": "^11.1.0"
  }
}
